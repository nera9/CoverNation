@model CoverNation.ViewModel.CoverManagmentViewModel.CoverShowList

@foreach (var item in Model.CoverList)
{

    <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3 videoContainer" id="@item.CoverId-replace">
        @{Html.RenderPartial("_cover", item);}
    </div><!-- end of videoContainer -->
            }
<script>
    $(document).ready(function () {
        setHoverEffect("coverVideo", "playBtn");
    });
</script>
